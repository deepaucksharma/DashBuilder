# Copy this file to .env and fill in your values

# New Relic API Configuration
NEW_RELIC_API_KEY=your-new-relic-user-api-key
NEW_RELIC_ACCOUNT_ID=your-account-id
NEW_RELIC_REGION=US
NEW_RELIC_INGEST_KEY=your-ingest-key-optional

# NRDOT v2 Configuration
NRDOT_PROFILE=Conservative
NRDOT_TARGET_COVERAGE=95
NRDOT_COST_REDUCTION_TARGET=40

# Optional Configuration
NR_GUARDIAN_CACHE_TTL=3600
NR_GUARDIAN_RATE_LIMIT_MAX=25
NR_GUARDIAN_LOG_LEVEL=info
NR_GUARDIAN_ENABLE_CACHE=true